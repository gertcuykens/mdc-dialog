{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "sourceMap": false,
    "outDir": "dist",
    "removeComments": true,
    "stripInternal": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "types": []
  },
  "exclude": []
}
